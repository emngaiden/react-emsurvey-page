{
    "presets": [
        [
            "@babel/preset-env",
            {
                "targets": {
                    "esmodules": true
                }
            }
        ],
        "@babel/preset-typescript",
        "@babel/preset-react"
    ],
    "plugins": [
        [
            "module-resolver",
            {
                "alias": {
                    "src": "./src"
                }
            }
        ]
    ]
}